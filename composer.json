{
    "name":        "arcanesoft/blog",
    "description": "Blog package for ARCANESOFT/Foundation.",
    "homepage":    "https://github.com/ARCANESOFT/Blog",
    "keywords":    [
        "blog", "posts", "tags", "foundation", "arcanedev", "arcanesoft", "cms"
    ],
    "authors":  [
        {
            "name":     "ARCANEDEV",
            "email":    "arcanedev.maroc@gmail.com",
            "homepage": "https://github.com/ARCANESOFT",
            "role":     "Developer"
        }
    ],
    "type":    "library",
    "license": "MIT",
    "require": {
        "php":                    ">=5.5.9",
        "arcanedev/support":      "~3.0",
        "arcanesoft/contracts":   "~1.0"
    },
    "require-dev": {
        "arcanesoft/foundation":  "~0.1",
        "orchestra/testbench":    "~3.1",
        "phpunit/phpcov":         "~2.0",
        "phpunit/phpunit":        "~4.0|~5.0"
    },
    "autoload": {
        "psr-4": {
            "Arcanesoft\\Blog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arcanesoft\\Blog\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
